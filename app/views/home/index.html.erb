<!--Start Script for View More Categories-->
<style>
.projects .LimitedDivs {    display:none;   }
#seeAllProjects {       transition: all 600ms ease-in-out;    -webkit-transition: all 600ms ease-in-out;
-moz-transition: all 600ms ease-in-out;    -o-transition: all 600ms ease-in-out;}

</style>




<script>


$(function () {
$(".projects .LimitedDivs").slice(0, 6).show();
$("#seeAllProjects").on('click', function (e) {
e.preventDefault();
$(".projects .LimitedDivs:hidden").slice(0, 6).slideDown();
if ($(".projects .LimitedDivs:hidden").length == 0) {
$("#seeAllProjects").fadeOut('slow');
}
$('html,body').animate({
scrollTop: $(this).offset().top
}, 1500);
});
});

$('a[href=#top]').click(function () {
$('body,html').animate({
scrollTop: 0
}, 600);
return false;
});

$(window).scroll(function () {
if ($(this).scrollTop() > 50) {
$('.totop a').fadeIn();
} else {
$('.totop a').fadeOut();
}
});



</script>

<!--End Script for View More Categories-->

   
<script>
  
  
  function like_unlike_function(project_id,user_id){
    
    //alert(project_id);
    //alert(user_id);
    
    $.ajax({
          
          type: "post",
          url: "/home",
          data: { project_id:project_id, user_id:user_id },
          success: function (resp)
          {
           
            if(resp == 1)
            {
               $('#heartlike_'+project_id).addClass('mydummycolor');
            }
            if(resp == 2)
            {
               $('#heartlike_'+project_id).removeClass('mydummycolor');
            }
          }
     });
    
    
  }
  
  
</script>

